<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dawei.silkroad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.BaseActivity" />
        <activity android:name=".dev.logic.LoginActivity" />
        <activity android:name=".dev.HomePagerActivity" />
        <activity android:name=".dev.logic.RegisterActivity" />
        <activity android:name=".dev.logic.ForgetPwdActivity" />
        <activity android:name=".dev.logic.ChangePwdActivity" />
        <activity android:name=".dev.PersonalCenterActivity" />
        <activity android:name=".dev.ArtistActivity" />
        <activity android:name=".dev.StoreActivity" />
        <activity android:name=".dev.CooperativeActivity"></activity>
    </application>

</manifest>